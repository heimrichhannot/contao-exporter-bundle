{
  "name": "heimrichhannot/contao-exporter-bundle",
  "description": "A backend module for exporting any contao entity to file.",
  "keywords": [
    "contao",
    "export",
    "backend",
    "bundle",
    "developer",
    "excel",
    "csv"
  ],
  "type": "contao-bundle",
  "homepage": "https://github.com/heimrichhannot/contao-exporter-bundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Heimrich & Hannot",
      "email": "digitales@heimrich-hannot.de",
      "homepage": "https://www.heimrich-hannot.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/heimrichhannot/contao-exporter-bundle/issues"
  },
  "require": {
    "php": "^8.0",
    "contao/core-bundle": "^4.13 || ^5.0",
    "box/spout": "^2.7",
    "heimrichhannot/contao-field-value-copier-bundle": "^1.1",
    "heimrichhannot/contao-multi-column-editor-bundle": "^1.2 | ^2.0",
    "heimrichhannot/contao-utils-bundle": "^2.207",
    "symfony/cache": "^4.4 || ^5.2 || ^6.0",
    "symfony/config": "^4.4 || ^5.4 || ^6.0",
    "symfony/console": "^4.4 || ^5.4 || ^6.0",
    "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
    "symfony/event-dispatcher": "^4.4 || ^5.4 || ^6.0",
    "symfony/event-dispatcher-contracts": "^1||^2||^3",
    "symfony/http-kernel": "^4.4 || ^5.4 || ^6.0",
    "symfony/http-foundation": "^4.4 || ^5.4 || ^6.0",
    "symfony/translation": "^4.4 || ^5.4 || ^6.0",
    "symfony/translation-contracts": "^1.1.6||^2||^3"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\ContaoExporterBundle\\": "src/",
      "HeimrichHannot\\Exporter\\": "legacy/"
    }
  },
  "replace": {
    "heimrichhannot/contao-exporter": "*"
  },
  "suggest": {
    "mpdf/mpdf": "^7.0 for pdf export"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\ContaoExporterBundle\\ContaoManager\\Plugin"
  }
}
